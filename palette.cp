/* CSS HEX */
--navajo-white: #FEE2BCff;
--fawn: #E3A66Bff;
--sunset: #ECCEA7ff;
--kobicha: #723C21ff;
--raw-umber: #906C4Eff;

/* CSS HSL */
--navajo-white: hsla(35, 97%, 87%, 1);
--fawn: hsla(30, 68%, 65%, 1);
--sunset: hsla(34, 64%, 79%, 1);
--kobicha: hsla(20, 55%, 29%, 1);
--raw-umber: hsla(27, 30%, 44%, 1);

/* SCSS HEX */
$navajo-white:#fee2bc;
$fawn:#e3a66b;
$sunset:#eccea7;
$kobicha:#723c21;
$raw-umber:#906c4e;

/* SCSS HSL */
$navajo-white: hsla(35, 97%, 87%, 1);
$fawn: hsla(30, 68%, 65%, 1);
$sunset: hsla(34, 64%, 79%, 1);
$kobicha: hsla(20, 55%, 29%, 1);
$raw-umber: hsla(27, 30%, 44%, 1);

/* SCSS RGB */
$navajo-white: rgba(254, 226, 188, 1);
$fawn: rgba(227, 166, 107, 1);
$sunset: rgba(236, 206, 167, 1);
$kobicha: rgba(114, 60, 33, 1);
$raw-umber: rgba(144, 108, 78, 1);

/* SCSS Gradient */
$gradient-top: linear-gradient(0deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-right: linear-gradient(90deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-bottom: linear-gradient(180deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-left: linear-gradient(270deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-top-right: linear-gradient(45deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-bottom-right: linear-gradient(135deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-top-left: linear-gradient(225deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-bottom-left: linear-gradient(315deg, #FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);
$gradient-radial: radial-gradient(#FEE2BCff, #E3A66Bff, #ECCEA7ff, #723C21ff, #906C4Eff);